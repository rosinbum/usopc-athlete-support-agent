{
  "name": "usopc-athlete-support-agent",
  "private": true,
  "version": "0.0.1",
  "description": "USOPC Athlete Support Agent - AI-powered governance and compliance assistant for U.S. Olympic and Paralympic athletes",
  "packageManager": "pnpm@9.15.4",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "pnpm -r build",
    "dev": "sst dev",
    "test": "pnpm -r test",
    "typecheck": "pnpm -r typecheck",
    "clean": "pnpm -r clean",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down",
    "ingest": "pnpm --filter @usopc/ingestion ingest",
    "seed": "pnpm --filter @usopc/ingestion seed",
    "seed:pg": "pnpm --filter @usopc/ingestion seed:pg",
    "seed:dynamodb": "pnpm --filter @usopc/ingestion seed:dynamodb",
    "sources": "pnpm --filter @usopc/ingestion sources",
    "eval": "pnpm --filter @usopc/evals eval",
    "seed-langsmith": "pnpm --filter @usopc/evals seed-langsmith"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "prettier": "^3.4.2",
    "sst": "3.18.1",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  },
  "pnpm": {
    "overrides": {
      "zod-to-json-schema": "3.24.1",
      "@langchain/openai": "<1.0.0",
      "hono": ">=4.11.4",
      "@modelcontextprotocol/sdk": ">=1.25.2",
      "h3": ">=1.15.5",
      "axios": ">=1.13.5"
    }
  }
}
