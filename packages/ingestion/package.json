{
  "name": "@usopc/ingestion",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "ingest": "sst shell -- tsx src/scripts/ingest.ts",
    "seed": "sst shell -- tsx src/scripts/seed.ts",
    "seed:pg": "sst shell -- tsx src/scripts/seed-db.ts",
    "seed:dynamodb": "sst shell -- tsx src/scripts/seed-dynamodb.ts",
    "sources": "sst shell -- tsx src/scripts/sources-cli.ts",
    "discovery:run": "sst shell -- tsx src/scripts/discoveryCli.ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.998.0",
    "@aws-sdk/client-s3": "^3.998.0",
    "@aws-sdk/client-ses": "^3.998.0",
    "@aws-sdk/client-sqs": "^3.998.0",
    "@langchain/anthropic": "^1.3.20",
    "@langchain/community": "^1.1.19",
    "@langchain/core": "^1.1.28",
    "@langchain/openai": "^1.2.10",
    "@langchain/textsplitters": "^1.0.1",
    "@tavily/core": "^0.7.1",
    "@usopc/core": "workspace:*",
    "@usopc/shared": "workspace:*",
    "cheerio": "^1.0.0",
    "pdf-parse": "^2.4.5",
    "pg": "^8.19.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@smithy/util-stream": "^3.0.0",
    "@types/aws-lambda": "^8.10.160",
    "@types/pg": "^8.11.10",
    "sst": "4.0.6",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  }
}
